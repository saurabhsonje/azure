@model IEnumerable<EmployeeManagementSystemApplication.Models.RegistrationModel>

@{
    ViewBag.Title = "GetEmployees";
}

<h2>Employee Details</h2>
<div align="right">
    <input type="button" value="Logout" onclick="location.href='@Url.Action("Logout", "MainCommonMvc")'" />
</div>

<table class="table">
    <tr>
        <th>
            @Html.DisplayName("EmailID")
        </th>
        <th>
            @Html.DisplayName("User Name")
        </th>
        
        <th>
            @Html.DisplayName("Role")
        </th>
        <th>
            @Html.DisplayName("Studio Name")
        </th>
        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.EmailID)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserName)
        </td>
        
        <td>
            @Html.DisplayFor(modelItem => item.Role)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.StudioName)
        </td>
       
        <td>
            @Html.ActionLink("Edit", "EditEmployee", new { id=item.EmployeeID }) 
    </tr>
}

</table>
