@model EmployeeManagementSystemApplication.Models.StudioModel

@{
    ViewBag.Title = "GetStudioByID";
}

<h2>Studio Details</h2>
<div align="right">
    <input type="button" value="Logout" onclick="location.href='@Url.Action("Logout", "MainCommonMvc")'" />
</div>
<div>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.DisplayName("Studio Name")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudioName)
        </dd>

        <dt>
            @Html.DisplayName("Studio Description")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.StudioDescription)
        </dd>

        <dt>
            @Html.DisplayName("Tech Director")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.TechDirector)
        </dd>

        

    </dl>
</div>
<p>
    @Html.ActionLink("Edit", "UpdateStudio", new { id = Model.StudioID }) |
    @Html.ActionLink("Go to Studio List", "GetStudioList")
</p>
